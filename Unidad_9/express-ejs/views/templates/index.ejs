            <% personas.forEach((x) => { %>
                <tr>
                    <th scope="row"><%= x.cc %></th>
                    <td><%= x.name %></td>
                    <td><%= x.lastname %></td>
                    <td><%= x.email %></td>
                </tr>
            <% }); %>
        </tbody>
      </table>
